.container {
    width: 100%;
    height: 100rpx;
    display: flex;
    flex-direction: row;
    background: #fff;
}

.sleeve {
    font-family: iconfont !important;
    color: #157658 !important;
}

.icons {
    box-sizing: border-box;
    padding-left: 16rpx;
    padding-right: 22rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-around;
    width: 230rpx;
    height: 100%;
    color: #999;
}

.icons > view {
    width: 50%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.icons > view:nth-child(2) {
    margin-top: 8rpx;
    width: 50%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.icon text {
    font-size: 20rpx;
}
.cart{
    display: flex;
    align-items: center;
    justify-content: center;
    width: 260rpx;
    background-color: #333;
    height: 100%;
    color: #fff;

}
.buy{
    display: flex;
    align-items: center;
    justify-content: center;
    width: 260rpx;
    background: #157658;
    height: 100%;
    color: #fff;
}