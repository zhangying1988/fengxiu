.container {
    height: 320rpx;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    background: #fff;
}

.inner-container {
    width: 730rpx;
    height: 300rpx;
    border: 1px dashed #DECBE6;
}

.grid-item {
    /*width: 200rpx;*/
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.img {
    width: 60rpx;
    height: 60rpx;
}

.text {
    margin-top: 6rpx;
    font-size: 24rpx;
    color: #333;
}